<h2>Make a Square</h2>

<form action="/squares" method='post'>
  <%= token_tag %>
  <input type="text" name='square[side_length]' value'type side length'>
  <%= token_tag %>
  <input type="text" name='square[border_radius]' value'type border radius'>
  <%= token_tag %>
  <input type="text" name='square[color]' value'type color'>
  <input type="submit">
</form>
