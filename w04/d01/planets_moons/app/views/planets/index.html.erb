<h1>Planets</h1>

<ul>
  <% @planets.each do |planet| %>
    <li><a href='/planets/<%= planet.id %>'><%= planet.name %></a>
      <ul>
        <% planet.moons.each do |moon| %>
          <li><%= moon.name %></li>
        <% end %>
      </ul>
    </li>
  <%end%>
</ul>

<a href='/planets/new'>Make A Planet</a>
